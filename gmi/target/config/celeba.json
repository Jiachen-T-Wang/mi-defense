{
  "dataset":{
    "name":"celeba",
    "train_file":"./attack_dataset/Celeba/faceset1000/train_list.txt",
    "test_file":"./attack_dataset/Celeba/faceset1000/test_list.txt",
    "model_name":"VGG16",
    "mode":"reg",
    "n_classes":1000,
    "gpus":"2"
  },
  "VGG16":{
    "epochs":50,
    "batch_size":64,
    "instance":4,
    "lr":1e-2,
    "momentum":0.9,
    "weight_decay":1e-4,
    "gamma":0.2,
    "adjust_epochs":[20, 35]
  },
  "FaceNet":{
    "epochs":30,
    "batch_size":64,
    "instance":4,
    "lr":1e-2,
    "momentum":0.9,
    "weight_decay":1e-4,
    "adjust_lr":[1e-3, 1e-4],
    "adjust_epochs":[15, 25]
  },
  "FaceNet64":{
    "epochs":50,
    "batch_size":64,
    "learning_rate":1e-2,
    "momentum":0.9,
    "weight_decay":1e-4,
    "lrdecay_epoch":10,
    "lrdecay":0.1
  },
  "IR152":{
    "epochs":40,
    "batch_size":64,
    "learning_rate":1e-2,
    "momentum":0.9,
    "weight_decay":1e-4,
    "lrdecay_epoch":10,
    "lrdecay":0.1
  },
  "IR50":{
    "epochs":40,
    "batch_size":64,
    "learning_rate":1e-2,
    "momentum":0.9,
    "weight_decay":1e-4,
    "lrdecay_epoch":10,
    "lrdecay":0.1
  }
}

